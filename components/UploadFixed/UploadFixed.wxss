/* components/UploadFixed/UploadFixed.wxss */
.upload-box {
  position: fixed;
  z-index: 1001;
  bottom: 100rpx;
  right: 20rpx;
  width: 140rpx;
  height: 140rpx;
  border: 1rpx solid #d9d9d9;
  border-radius: 50%;
  text-align: center;
  line-height: 140rpx;
  font-size: 30rpx;
}

.upload-box:active {
  background-color: #d9d9d9;
  color: #fff;
}